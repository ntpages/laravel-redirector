{
    "name": "ntpages/laravel-redirector",
    "description": "Laravel package for redirecting pages",
    "keywords": [
        "laravel",
        "redirect"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Anton Stasyuk",
            "email": "anton@ntpages.com.au"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "ext-curl": "*",
        "laravel/framework": "^5.7.25|^6|^7|^8"
    },
    "autoload": {
        "psr-4": {
            "Ntpages\\LaravelRedirector\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Ntpages\\LaravelRedirector\\Provider"
            ]
        }
    },
    "scripts": {
        "post-install-cmd": []
    }
}
